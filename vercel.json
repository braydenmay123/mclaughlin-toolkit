{
  "buildCommand": "npx expo export -p web",
  "outputDirectory": "dist",
  "framework": null,
  "routes": [
    { "src": "/_expo/(.*)", "dest": "/_expo/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}